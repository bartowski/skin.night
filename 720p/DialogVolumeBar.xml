<!--Alaska-->
<window id="104">
	<animation type="WindowOpen" reversible="false">
		<effect type="zoom" start="80" end="100" center="1580,125" delay="200" easing="out" tween="back" time="300" />
		<effect type="fade" delay="200" start="0" end="100" time="300" />
	</animation>
	<animation type="WindowClose" reversible="false">
		<effect type="zoom" start="100" end="80" center="1580,125" easing="in" tween="back" time="300" />
		<effect type="fade" start="100" end="0" time="300" />
	</animation>
	<controls>
		<control type="group">
			<posx>880</posx>
			<posy>90</posy>
			<width>707</width>
			<height>183</height>
			<control type="group">
				<control type="image">
					<posx>322</posx>
					<posy>10</posy>
					<width>77</width>
					<height>342</height>
					<texture>dialogs/volume_back.png</texture>
				</control>
				<control type="progress" id="1">
					<description>Progressbar</description>
					<posx>226</posx>
					<posy>160</posy>
					<width>240</width>
					<height>28</height>
					<info>Player.Volume</info>
					<lefttexture>-</lefttexture>
					<midtexture>dialogs/osd_volume.png</midtexture>
					<righttexture>-</righttexture>
					<overlaytexture>-</overlaytexture>
					<visible>true</visible>
					<animation effect="rotate" center="auto" start="90" end="90" time="0" condition="true">Conditional</animation>
				</control>

				<control type="image">
					<posx>322</posx>
					<posy>10</posy>
					<width>77</width>
					<height>342</height>
					<texture>dialogs/volume_cover.png</texture>
