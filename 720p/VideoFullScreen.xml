<!--Alaska-->

<window id="2005">
	<controls>
		<control type="label">
			<include>WindowID</include>
			<label>VideoFullScreen.xml</label>
			<visible>Skin.HasSetting(debugmode)</visible>
		</control>
	<!-- codec infos -->
		<control type="group">
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>125</height>
			<visible>Control.IsVisible(10)</visible>
			<animation type="Visible">
				<effect type="fade" time="400" start="0" end="100" />
				<effect type="slide" time="400" start="0,-125" end="0,0" easing="out" tween="cubic" />
			</animation>
			<animation type="Hidden">
				<effect type="fade" time="400" start="100" end="0" />
				<effect type="slide" time="400" start="0,0" end="0,-125" easing="in" tween="cubic" />
			</animation>
			<animation type="WindowOpen">
				<effect type="fade" time="400" start="0" end="100" />
				<effect type="slide" time="400" start="0,-125" end="0,0" easing="out" tween="cubic" />
			</animation>
			<animation type="WindowClose">
				<effect type="fade" time="400" start="100" end="0" />
				<effect type="slide" time="400" start="0,0" end="0,-125" easing="in" tween="cubic" />
			</animation>
			<control type="image">
				<posx>-5</posx>
				<posy>0</posy>
				<width>1290</width>
				<height>165</height>
				<texture flipx="true">common/white.png</texture>
				<colordiffuse>BackgroundColor</colordiffuse>
				<aspectratio aligny="bottom">scale</aspectratio>
			</control>
			<control type="image">
				<posx>-5</posx>
				<posy>0</posy>
				<width>1290</width>
				<height>165</height>
				<texture flipx="true">backdrops/filter_2.png</texture>
				<colordiffuse>BackgroundColorDiffuse</colordiffuse>
				<aspectratio aligny="bottom">scale</aspectratio>
			</control>
			<control type="label" id="10">
				<description>row 1 label</description>
				<posx>0</posx>
				<posy>12</posy>
				<label>-</label>
				<align>center</align>
				<width>1280</width>
				<textcolor>ListTitleNF</textcolor>
				<font>Font_Date</font>
			</control>
			<control type="label" id="11">
				<description>row 2 label</description>
				<posx>0</posx>
				<posy>37</posy>
				<label>-</label>
				<align>center</align>
				<width>1280</width>
				<textcolor>ListTitleNF</textcolor>
				<font>Font_Date</font>
			</control>
			<control type="label" id="12">
				<description>row 3 label</description>
				<posx>0</posx>
				<posy>62</posy>
				<label>-</label>
				<align>center</align>
				<width>1280</width>
				<textcolor>ListTitleNF</textcolor>
				<font>Font_Date</font>
			</control>
		</control>
	<!-- Buffering Dialog -->
		<control type="label" id="24">
			<description>buffering label</description>
			<posx>210</posx>
			<posy>520</posy>
			<label>15107</label>
