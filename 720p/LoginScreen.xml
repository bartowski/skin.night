<!--Empty-->

<window id="29">
	<defaultcontrol always="true">52</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	
	<coordinates>
		<system>1</system>
		<posx>360</posx>
		<posy>120</posy>
	</coordinates>

	<controls>

		<control type="image" id="0">
			<description>Back Panel</description>
			<posx>-30</posx>
			<posy>-30</posy>
			<width>620</width>
			<height>500</height>
			<texture>favourites/favourites_panel.png</texture>
			<colordiffuse>FavListPanel</colordiffuse>
		</control>
		
		<control type="label">
			<include>WindowID</include>
			<label>LoginScreen.xml</label>
			<visible>Skin.HasSetting(debugmode)</visible>
		</control>

		<control type="wraplist" id="52">
			<description>Login Wraplist</description>
			<posx>0</posx>
			<posy>50</posy>
			<width>720</width>
			<height>500</height>
			<visible>-</visible>
			<onup>300</onup>
			<ondown>300</ondown>
			<onleft>300</onleft>
			<onright>300</onright>
			<focusposition>5</focusposition>
			<scrolltime>200</scrolltime>
			<itemlayout height="50">
				<control type="label">
					<posx>360</posx>
					<width>250</width>
					<info>ListItem.Label</info>
					<textcolor>Text</textcolor>
					<scroll>false</scroll>
					<align>center</align>
				</control>
			</itemlayout>
			<focusedlayout height="50">
				<control type="label">
					<posx>360</posx>
					<width>250</width>
					<info>ListItem.Label</info>
					<align>center</align>
				</control>
			</focusedlayout>
		</control>

		<include>Object_DebugGrid</include>

	</controls>

</window>